{
    "name": "NGINX Web Server",
    "version": "1.25.3-alpine3.18-slim",
    "slug": "nginx_ws",
    "description": "Nginx Web Server based on Alpine Linux",
    "url": "https://github.com/hulkhaugen/hassio-addons/tree/master/nginx_ws",
    "arch": [
        "armhf",
        "armv7",
        "aarch64",
        "amd64",
        "i386"
    ],
    "init": false,
    "webui": "http://[HOST]:[PORT:8000]",
    "ports": {
        "80/tcp": 8000
    },
    "ports_description": {
        "80/tcp": "HTTP Interface"
    },
    "map": [
        "addon_config:rw"
    ]
}