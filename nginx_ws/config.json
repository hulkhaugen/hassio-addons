{
    "name": "NGINX Web Server",
    "version": "1.25.3-alpine-slim",
    "slug": "nginx_ws",
    "description": "NGINX Web Server based on Alpine Slim. Additional APK packages and PyPI dependencies can be added with .txt-files. Cronjobs can be setup in the config folder.",
    "url": "https://github.com/hulkhaugen/hassio-addons/tree/master/nginx_ws",
    "arch": [
        "armhf",
        "armv7",
        "aarch64",
        "amd64",
        "i386"
    ],
    "init": false,
    "webui": "http://[HOST]:[PORT:8765]",
    "ports": {
        "80/tcp": 8765
    },
    "ports_description": {
        "80/tcp": "HTTP Interface"
    },
    "map": [
        "addon_config:rw",
        "media:rw",
        "share:rw"
    ],
    "backup_exclude": [
        "**/__pycache__",
        "**/www/temp",
        "**/5ec262d9_nginx_ws/log"
    ]
}